{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Flutter Build Runner",
      "type": "shell",
      "command": "flutter pub run build_runner build --delete-conflicting-outputs",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    },
    {
      "label": "Flutter Generate Localization",
      "type": "shell",
      "command": "flutter gen-l10n",
      "group": {
        "kind": "build",
        "isDefault": false
      },
      "problemMatcher": []
    }
  ]
}
